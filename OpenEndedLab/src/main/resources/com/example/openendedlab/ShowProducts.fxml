<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.openendedlab.ShowProducts.ShowProductsController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>

   <TilePane fx:id="tilePane" hgap="10" vgap="10" prefColumns="3" prefHeight="500.0" prefWidth="500.0">
      <!-- TilePane content will be added dynamically -->
   </TilePane>

   <Button fx:id="btnBack" mnemonicParsing="false" onAction="#GoBack" text="BACK">
      <font>
         <Font name="Arial Black" size="18.0"/>
      </font>
   </Button>
</VBox>
